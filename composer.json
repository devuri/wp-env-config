{
    "name": "devuri/wp-env-config",
    "type": "library",
    "description": "A small package to define WordPress configuration constants using environment variables with a .env file.",
    "keywords": [
        "wp",
        "wordpress",
        "wp-config",
        "environment",
        "configuration",
        "config",
        "env",
        "dotenv",
        "devuri"
    ],
    "homepage": "https://github.com/devuri/wp-env-config",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Uriel Wilson",
            "email": "support@urielwilson.com",
            "homepage": "https://urielwilson.com",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/devuri/wp-env-config",
        "issues": "https://github.com/devuri/wp-env-config/issues"
    },
    "require": {
        "php": "^7.3 || ^7.4 || ^8.0",
        "vlucas/phpdotenv": "^5.2",
        "roots/wp-config": "1.0.0",
        "oscarotero/env": "^2.1",
        "symfony/var-dumper": "^5.1.4",
        "symfony/error-handler": "^5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/var-dumper": "^5.3",
        "vimeo/psalm": "^4.8",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "DevUri\\Config\\": "src"
        }
    },
    "scripts": {
        "nodev": "composer install --no-dev",
        "phploc": "php ./bin/phploc.phar src > phploc-result.txt",
        "phpdoc": "php ./bin/phpdoc.phar",
        "phpcs": "vendor/bin/php-cs-fixer fix src",
        "psalm": "vendor/bin/psalm > psalm-result.txt",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
      }
}
